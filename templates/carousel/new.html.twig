{% extends 'base.html.twig' %}

{% block title %}New Carousel{% endblock %}

{% block body %}
    <h1>Create new Carousel</h1>
    <section>
        <div class="card_dash">
        {{ include('carousel/_form.html.twig') }}
        {% for message in app.flashes('error') %}
